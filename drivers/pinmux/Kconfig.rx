config PINMUX_RX
	bool "Pinmux driver for RX65N series MCU"
	depends on SOC_FAMILY_RX
	help
	  Enable driver for the Renesas RX65N SOC pinmux driver

config PINMUX_RX_NAME
	string "Renesas RX65N pinmux driver name"
	default "pinmux"
	depends on PINMUX_RX
